{
  "name": "inlineweb-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "clear:babel-cache": "rimraf -rf ./node_modules/.cache/babel-loader/*",
    "copy:dist": "cpy '**/*.*' '!index.html' '../../api/public' --cwd=dist --parents",
    "copy:index": "cpy 'dist/index.html' '../api/resources/views' --rename=vue.blade.php",
    "copy:all": "run-p copy:dist copy:index",
    "build:copy": "run-s build copy:all"
  },
  "dependencies": {
    "@emmetio/codemirror-plugin": "^0.5.4",
    "js-beautify": "^1.8.9",
    "lzma": "^2.3.2",
    "normalize-scss": "^7.0.1",
    "typeface-montserrat": "0.0.54",
    "vee-validate": "^2.1.5",
    "vue": "^2.5.17",
    "vue-codemirror": "^4.0.6",
    "vue-router": "^3.0.1",
    "vue-truncate-filter": "^1.1.7",
    "vuejs-paginate": "^2.1.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.0",
    "@vue/cli-service": "^3.2.0",
    "@vue/eslint-config-prettier": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cpy-cli": "^2.0.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  }
}
